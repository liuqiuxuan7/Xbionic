<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
android:background="@drawable/allbackground" 
android:layout_width="fill_parent" 
android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout 
	android:id="@id/relativeLayout1" 
	android:background="@drawable/topnav" 
	android:layout_width="fill_parent" 
	android:layout_height="53.0dip">
        <Button 
		android:id="@+id/btn_backmaseter" 
		android:background="@drawable/backbtn" 
		android:layout_width="53.0dip" 
		android:layout_height="fill_parent" />
        <TextView 
		android:textSize="22.0sp" 
		android:textColor="#ffd7d7d7" 
		android:gravity="center" 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent" 
		android:text="达人申请" />
        <Button 
		android:id="@+id/but_mastereditor" 
		android:background="@drawable/pencil"
		android:layout_width="53.0dip" 
		android:layout_height="fill_parent" 
		android:layout_alignParentRight="true" />
    </RelativeLayout>
    <RelativeLayout 
	android:id="@id/relativeLayout2" 
	android:background="@drawable/expert_edit1" 
	android:layout_width="fill_parent" 
	android:layout_height="wrap_content" 
	android:layout_marginLeft="8.0dip" 
	android:layout_marginTop="18.0dip" 
	android:layout_marginRight="8.0dip" 
	android:layout_below="@id/relativeLayout1" 
	android:layout_alignParentLeft="true">
        <TextView 
		android:textSize="17.0dip" 
		android:id="@+id/tv_maseter1" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_marginLeft="16.0dip" 
		android:layout_marginTop="10.0dip" 
		android:text="姓氏:" />
        <EditText
		android:id="@id/editText1" 
		android:background="@null" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" 
		android:ems="10" 
		android:layout_toRightOf="@+id/tv_maseter1"
		android:layout_alignBaseline="@+id/tv_maseter1" 
		android:layout_alignBottom="@+id/tv_maseter1" 
		android:inputType="textPersonName">
            <requestFocus />
        </EditText>
        <TextView
		android:textSize="17.0dip" 
		android:id="@+id/tv_maseter2" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" 
		android:layout_marginLeft="16.0dip" 
		android:text="名字:" 
		android:layout_alignParentLeft="true" 
		android:layout_centerVertical="true" />
        <TextView 
		android:textSize="17.0dip" 
		android:id="@id/textView1" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_marginTop="120.0dip" 
		android:text="Email:"
		android:layout_below="@+id/tv_maseter2"
		android:layout_alignLeft="@+id/tv_maseter2" />
        <EditText 
		android:background="@null" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:ems="10" 
		android:layout_alignLeft="@id/editText1" 
		android:layout_centerVertical="true" 
		android:inputType="textPersonName" />
        <EditText 
		android:background="@null" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" 
		android:ems="10" 
		android:layout_toRightOf="@id/textView1" 
		android:layout_alignBaseline="@id/textView1"
		android:layout_alignBottom="@id/textView1" 
		android:inputType="textPersonName" />
    </RelativeLayout>
    <RelativeLayout 
	android:id="@id/relativeLayout3"
	android:background="@drawable/expert_edit2" 
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="16.0dip" 
	android:layout_below="@id/relativeLayout2"
	android:layout_alignLeft="@id/relativeLayout2"
	android:layout_alignRight="@id/relativeLayout2">
        <TextView
		android:textSize="17.0dip" 
		android:id="@id/textView2" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="16.0dip" 
		android:layout_marginTop="10.0dip"
		android:text="手机号码:" />
        <EditText 
		android:background="@null" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" 
		android:ems="10" 
		android:layout_toRightOf="@id/textView2" 
		android:layout_alignBaseline="@id/textView2" 
		android:layout_alignBottom="@id/textView2"
		android:inputType="textPersonName" />
        <TextView 
		android:textSize="17.0dip" 
		android:id="@id/textView3" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:layout_marginTop="50.0dip"
		android:text="推荐人手机号码:" 
		android:layout_below="@id/textView2"
		android:layout_alignLeft="@id/textView2" />
        <EditText
		android:background="@null" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:ems="10" 
		android:layout_toRightOf="@id/textView3" 
		android:layout_alignBaseline="@id/textView3" 
		android:layout_alignBottom="@id/textView3" 
		android:layout_alignParentRight="true" 
		android:inputType="textPersonName" />
    </RelativeLayout>
    <Button 
	android:textSize="17.0dip" 
	android:textColor="#ffffffff" 
	android:id="@id/button1" 
	android:background="@drawable/go_tribe_2"
	android:layout_width="wrap_content" 
	android:layout_height="42.0dip" 
	android:layout_marginTop="25.0dip" 
	android:text="确认提交" 
	android:layout_below="@+id/relativeLayout4" 
	android:layout_alignLeft="@+id/relativeLayout4"
	android:layout_alignRight="@+id/relativeLayout4" />
    <RelativeLayout
	android:id="@+id/relativeLayout4" 
	android:background="@drawable/expert_edit3"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content" 
	android:layout_marginTop="16.0dip"
	android:layout_below="@id/relativeLayout3"
	android:layout_alignLeft="@id/relativeLayout3" 
	android:layout_alignRight="@id/relativeLayout3">
        <TextView 
		android:textSize="17.0dip" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:layout_marginLeft="16.0dip" 
		android:layout_marginTop="10.0dip"
        android:text="资历" />
    </RelativeLayout>
</RelativeLayout>
